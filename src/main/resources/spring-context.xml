<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
       xmlns="http://www.springframework.org/schema/beans"
       xsi:schemaLocation="http://www.springframework.org/schema/beans
        http://www.springframework.org/schema/beans/spring-beans.xsd">

    <bean id="csvFileReader" class="ru.otus.agaryov.dz1.csvfilereader.CsvFileReader">
        <constructor-arg index="0" value="./QuestionsAndAnswers.csv"/>
    </bean>
    <bean id="checker" class="ru.otus.agaryov.dz1.results.ResultChecker">
        <constructor-arg name="csvFileReader" ref="csvFileReader"/>
    </bean>
</beans>
